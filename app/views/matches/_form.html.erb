<%= form_for @match, :url => {:action => "update"} do |form| %>
	<div class="form-group">
		<%= form.text_field :status, placeholder: 'Status', class: 'form-control' %>
	</div>

	<div class="form-group">
		<%= form.text_field :winner, placeholder: 'Winner', class: 'form-control' %>
	</div>

	<div class="actions">
		<%= form.submit %>
	</div>
<% end %>